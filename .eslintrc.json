{
    "extends": "eslint:recommended",
    "env": {
        "node": true,
        "browser": true,
        "jquery": true,
        "es6": true
    },
    "globals": {
        "console": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "no-console": 0
    }
}